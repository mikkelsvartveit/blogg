{
  "name": "svelte-pocketbase-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:sveltekit": "pnpm run typegen && vite dev",
    "dev:pocketbase": "cd pocketbase && ./pocketbase serve",
    "dev": "pnpm run '/^dev:.*/'",
    "build": "pnpm check && vite build",
    "preview": "vite preview",
    "typegen": "pocketbase-typegen --db ./pocketbase/pb_data/data.db --out src/lib/pocketbase-typegen.ts",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.1",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "pocketbase": "^0.25.1",
    "pocketbase-typegen": "^1.3.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.19.3",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.0.11"
  },
  "type": "module"
}
