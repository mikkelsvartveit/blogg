{
  "name": "svelte-pocketbase-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:sveltekit": "pnpm run typegen && vite dev",
    "dev:pocketbase": "cd pocketbase && ./pocketbase serve",
    "dev": "pnpm run '/^dev:.*/'",
    "build": "pnpm check && vite build",
    "preview": "vite preview",
    "typegen": "pocketbase-typegen --db ./pocketbase/pb_data/data.db --out src/lib/pocketbase-typegen.ts",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/kit": "^2.36.2",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "pocketbase": "^0.26.2",
    "pocketbase-typegen": "^1.3.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.38.5",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3"
  },
  "type": "module"
}
